<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.gr.grquickrescue.models">
	<class name="Address" table="Address">
		<id name="id" type="int" column="id">
			<generator class="native"></generator>
		</id>
		<property name="streetAddress" type="java.lang.String" column="streetAddress"/>
		<property name="city" type="java.lang.String" column="city"/>
		<property name="province" type="java.lang.String" column="province"/>
		<property name="country" type="java.lang.String" column="country"></property>
		<property name="latitude" type="double" column="latitude"/>
		<property name="longitude" type="double" column="longitude"/>
	</class>
</hibernate-mapping>